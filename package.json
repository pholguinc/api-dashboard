{
  "name": "telemetro-backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "dev-force": "ts-node --transpile-only --ignore-diagnostics src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "seed-data": "node scripts/seed-data.js",
    "seed-ads": "node scripts/seed-ads.js",
    "seed-clips": "node scripts/seed-clips.js",
    "seed-banners": "node scripts/seed-banners.js",
    "seed-microcourses": "node scripts/seed-microcourses.js",
    "test-connections": "node scripts/test-connections.js",
    "setup": "node scripts/setup-complete.js",
    "init-system": "node scripts/initialize-complete-system.js",
    "verify": "node scripts/verify-integration.js",
    "verify-complete": "node scripts/verify-complete-system.js",
    "seed-voto-seguro": "node scripts/seed-voto-seguro.js",
    "streaming-server": "ts-node src/streaming-server.ts",
    "deploy-streaming": "./scripts/deploy-streaming-infrastructure.sh",
    "init-radio-ai": "node scripts/init-radio-ai.js",
    "dev-radio-ai": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --env-file .env.radio-ai src/server.ts"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.35.0",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.0",
    "firebase-admin": "^12.7.0",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "node-media-server": "^2.7.4",
    "sharp": "^0.34.3",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^4.23.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/aws-sdk": "^0.0.42",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.12",
    "@types/node-media-server": "^2.3.7",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "eslint": "^9.8.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
